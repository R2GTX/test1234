<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_company_form_midav">
            <field name="name">res.company.form.midav</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="priority" eval="20"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[1]//field[@name='report_header']" position="after">
                        <field name="approval"/>
                        <field name="fda"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>